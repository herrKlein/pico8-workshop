-- map

function _init()
 plr = {
  pos={x=10, y=10},
  move = {
   vy = 0,
   bounce = -1
  }
 }
	map_x = 0
end

function _update60()
 if btn(4) and nrg > 0 then 
  plr.pos.y -= flap
  plr.move.vy = 0
  nrg -= 1
 else
  plr.move.vy += grav
  plr.pos.y += plr.move.vy
  if plr.pos.y > ground then 
   plr.move.vy *= plr.move.bounce
   plr.pos.y = ground
   plr.move.vy += fri
   nrg = 50
  end
 end
 -- plr.pos.x += 1 -- 4
 map_x -= 1  -- 3
end

function _draw()
 cls()
 print(nrg,0,0,3)
 map(0, 0, map_x, 0, 128, 16) -- 1
 -- map(0, 0, map_x+128, 0, 16, 16)
 -- camera(cam.x, cam.y)
 spr(1, plr.pos.x, plr.pos.y, 2, 2)
end
