-- player

function init_player()
	plr = {
	  x=64,
	  y=64,
	  grv=0.1,
	  vy=0,
	  w=16,
	  h=16,
	  fri=1
	}
end

function update_player()
 if btnp(2) then 
		plr.vy = -2
	end
	
	plr.vy += plr.grv
	plr.y += plr.vy
	
	local ground = 128 - plr.h
	if plr.y > ground then
		plr.vy *= -1
		plr.y = ground
		plr.vy += plr.fri
	end
end

function draw_player()
 spr(1,plr.x,plr.y,2,2)
end
